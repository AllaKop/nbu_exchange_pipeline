{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowAllCloudWatchActionsOnTaggedResources",
      "Effect": "Allow",
      "Action": [
        "logs:*",
        "cloudwatch:*"
      ],
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "aws:ResourceTag/project_name": "nbu-exchange"
        }
      }
    }
  ]
}